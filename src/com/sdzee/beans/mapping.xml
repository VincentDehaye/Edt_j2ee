<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.sdzee.beans.CreneauEntity">
        <table name="Creneau" schema="sr03_project" catalog=""/>
        <attributes>
            <id name="idCreneau">
                <column name="IdCreneau"/>
            </id>
            <basic name="uv">
                <column name="Uv"/>
            </basic>
            <basic name="horaire">
                <column name="Horaire"/>
            </basic>
            <basic name="salle">
                <column name="Salle"/>
            </basic>
            <basic name="type">
                <column name="Type"/>
            </basic>
            <basic name="groupe">
                <column name="Groupe"/>
            </basic>
            <basic name="alternance">
                <column name="Alternance"/>
            </basic>
            <many-to-one name="uvByUv" optional="true" target-entity="com.sdzee.beans.UvEntity">
                <join-column name="Uv" referenced-column-name="IdUv"/>
            </many-to-one>
            <many-to-one name="horaireByHoraire" optional="true" target-entity="com.sdzee.beans.HoraireEntity">
                <join-column name="Horaire" referenced-column-name="IdHoraire"/>
            </many-to-one>
            <one-to-many name="participationsByIdCreneau" mapped-by="creneauByCren"
                         target-entity="com.sdzee.beans.ParticipationEntity"/>
        </attributes>
    </entity>
    <entity class="com.sdzee.beans.EtudiantEntity">
        <table name="Etudiant" schema="sr03_project" catalog=""/>
        <attributes>
            <id name="idEtudiant">
                <column name="IdEtudiant"/>
            </id>
            <basic name="nom">
                <column name="Nom"/>
            </basic>
            <basic name="prenom">
                <column name="Prenom"/>
            </basic>
            <basic name="mail">
                <column name="Mail"/>
            </basic>
            <basic name="photo">
                <column name="Photo"/>
            </basic>
            <basic name="semestre">
                <column name="Semestre"/>
            </basic>
            <basic name="cursus">
                <column name="Cursus"/>
            </basic>
            <basic name="niveau">
                <column name="Niveau"/>
            </basic>
            <one-to-many name="participationsByIdEtudiant" mapped-by="etudiantByEtu"
                         target-entity="com.sdzee.beans.ParticipationEntity"/>
        </attributes>
    </entity>
    <entity class="com.sdzee.beans.HoraireEntity">
        <table name="Horaire" schema="sr03_project" catalog=""/>
        <attributes>
            <id name="idHoraire">
                <column name="IdHoraire"/>
            </id>
            <basic name="heureDébut">
                <column name="Heure_début"/>
            </basic>
            <basic name="heureFin">
                <column name="Heure_fin"/>
            </basic>
            <basic name="jourSemaine">
                <column name="Jour_semaine"/>
            </basic>
            <one-to-many name="creneausByIdHoraire" mapped-by="horaireByHoraire"
                         target-entity="com.sdzee.beans.CreneauEntity"/>
        </attributes>
    </entity>
    <entity class="com.sdzee.beans.ParticipationEntity">
        <table name="Participation" schema="sr03_project" catalog=""/>
        <id-class class="com.sdzee.beans.ParticipationEntityPK"/>
        <attributes>
            <id name="etu">
                <column name="Etu"/>
            </id>
            <id name="cren">
                <column name="Cren"/>
            </id>
            <many-to-one name="etudiantByEtu" optional="true" target-entity="com.sdzee.beans.EtudiantEntity">
                <join-column name="Etu" referenced-column-name="IdEtudiant"/>
            </many-to-one>
            <many-to-one name="creneauByCren" optional="true" target-entity="com.sdzee.beans.CreneauEntity">
                <join-column name="Cren" referenced-column-name="IdCreneau"/>
            </many-to-one>
        </attributes>
    </entity>
    <entity class="com.sdzee.beans.ProfesseurEntity">
        <table name="Professeur" schema="sr03_project" catalog=""/>
        <attributes>
            <id name="idProfesseur">
                <column name="IdProfesseur"/>
            </id>
            <basic name="nom">
                <column name="Nom"/>
            </basic>
            <basic name="prenom">
                <column name="Prenom"/>
            </basic>
            <basic name="mail">
                <column name="Mail"/>
            </basic>
            <basic name="photo">
                <column name="Photo"/>
            </basic>
            <one-to-many name="uvsByIdProfesseur" mapped-by="professeurByResponsable"
                         target-entity="com.sdzee.beans.UvEntity"/>
        </attributes>
    </entity>
    <entity class="com.sdzee.beans.UvEntity">
        <table name="UV" schema="sr03_project" catalog=""/>
        <attributes>
            <id name="idUv">
                <column name="IdUv"/>
            </id>
            <basic name="identifiant">
                <column name="Identifiant"/>
            </basic>
            <basic name="responsable">
                <column name="Responsable"/>
            </basic>
            <basic name="description">
                <column name="Description"/>
            </basic>
            <many-to-one name="professeurByResponsable" optional="true"
                         target-entity="com.sdzee.beans.ProfesseurEntity">
                <join-column name="Responsable" referenced-column-name="IdProfesseur"/>
            </many-to-one>
            <one-to-many name="creneausByIdUv" mapped-by="uvByUv" target-entity="com.sdzee.beans.CreneauEntity"/>
        </attributes>
    </entity>
</entity-mappings>
